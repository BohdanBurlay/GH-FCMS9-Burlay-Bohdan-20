
{% block paragraph %}
  <section class="section-wrapper-teller">
    {% block content %}
      <h2>
        {% if content.field_section_title %}
        {{ content.field_section_title }}
        {% endif %}
      </h2>
      <div class="section-items">
        {% if content.field_items %}
        {{ content.field_items }}
        {% endif %}
      </div>
    {% endblock %}
  </section>
{% endblock paragraph %}
